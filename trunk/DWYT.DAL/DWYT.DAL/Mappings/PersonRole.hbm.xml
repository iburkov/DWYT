<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	assembly="DWYT.Core"
	namespace="DWYT.Core.DomainModels">

  <class name="PersonRole" table="PersonRoles">
    <id name="Id">
      <generator class="guid" />
    </id>
    <many-to-one name="Person" column="PersonId" class="DWYT.Core.DomainModels.Person"/>
    <many-to-one name="Role" column="RoleId" class="DWYT.Core.DomainModels.Role"/>
  </class>
</hibernate-mapping>