<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	assembly="DWYT.Core"
	namespace="DWYT.Core.DomainModels">

  <class name="Role" table="Roles">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="Name"/>
    <list name="PersonRoles" lazy="false">
      <key column="Id"/>
      <index column="RoleId"/>
      <one-to-many class="DWYT.Core.DomainModels.PersonRole"/>
    </list>
    <list name="PersonRoles" lazy="false">
      <key column="Id"/>
      <index column="RoleId"/>
      <one-to-many class="DWYT.Core.DomainModels.PersonRole"/>
    </list>
  </class>
</hibernate-mapping>