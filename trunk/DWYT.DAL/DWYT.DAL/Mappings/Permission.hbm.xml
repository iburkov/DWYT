<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	assembly="DWYT.Core"
	namespace="DWYT.Core.DomainModels">

  <class name="Permission" table="Permissions">
    <id name="Id">
      <generator class="identity" />
    </id>
    <property name="Name" />
    <list name="RolePermissions" lazy="false">
      <key column="Id"/>
      <index column="PermissionId"/>
      <one-to-many class="DWYT.Core.DomainModels.RolePermission"/>
    </list>
  </class>
</hibernate-mapping>